{
    "version": 2,
    "functions": {
      "api/APIs/**/*.php": {
        "runtime": "vercel-php@0.7.1"
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/APIs/$1"
      },
      {
        "src": "/static/(.*)",
        "dest": "/api/src/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/api/src/view/$1"
      }
    ]
  }
  