{
    "version": 2,
    "builds": [
      {
        "src": "public/**/*.php",
        "use": "@vercel/php"
      },
      {
        "src": "APIs/**/*.php",
        "use": "@vercel/php"
      },
      {
        "src": "server/main.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/APIs/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ]
  }
  